<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter
	xmlns="https://github.com/spotbugs/filter/3.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/4.0.0/spotbugs/etc/findbugsfilter.xsd">
	
	<!-- Ignore activators -->
	<Match>
		<Class name="~.*Activator" />
	</Match>
	<Match>
		<Class name="~.*LibPlugin" />
	</Match>
	
	<!-- Ignore generated java classes -->
	<Match>
		<Source name="~.*src-gen.*.java" />
	</Match>
	<Match>
		<Source name="~.*xtend-gen.*.java" />
	</Match>
	<Match>
		<Source name="~.*src-dmf.*.java" />
	</Match>
</FindBugsFilter>